<% @tasks.each do |task| %>
    <%= link_to '[x]',
        user_task_path(task.user, task),
        method: :delete,
        data: { confirm: 'Sure?' } %>
    <br>
    <%= task.user.name %>
    <br>
    <%= link_to task.title, user_task_path(task.user, task) %>
    <br>
    <%= task.body %> 
    <br>
    <br>
<% end %>
